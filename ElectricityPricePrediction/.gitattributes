# GitHub Language Statistics Configuration
# Only show actual source code, hide generated outputs

# Generated HTML reports and interactive plots
Analysis/**/report.html linguist-generated=true
Analysis/**/Plots/**/*.html linguist-generated=true

# Generated JSON results
Analysis/**/*.json linguist-generated=true

# Jupyter Notebooks (they contain lots of HTML/JSON output, not primary code)
*.ipynb linguist-generated=true

# Large data files
data/**/*.csv linguist-vendored=true
data/**/*.xls linguist-vendored=true

# Ensure Python is detectable as primary language
*.py linguist-detectable=true
